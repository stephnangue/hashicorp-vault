# config file version
apiVersion: 1

# list of datasources to insert/updata depending on what's available in the database
datasources:
    - name: InfluxDB_v2_Flux
      type: influxdb
      access: proxy
      url: http://localhost:8086
      jsonData:
        version: Flux
        organization: openlab
        httpMode: POST
        tlsSkipVerify: true
        defaultBucket: vault_infra
      secureJsonData:
        token: {{ influxdb_token }}
      editable: true
      readOnly: false